{
    "name": "ffnlabs/avmediahelpers",
    "type": "library",
    "description": "Helper enums and methods to working with audiovisual content metadata",
    "keywords": [
        "ffnlabs",
        "avmediahelpers"
    ],
    "homepage": "https://github.com/ffnlabs/avmediahelpers",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Marconi",
            "email": "hello@559labs.com",
            "homepage": "https://559labs.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "FFNLabs\\AVMediaHelpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FFNLabs\\AVMediaHelpers\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
